// Enum para definir los tipos de items
enum ItemType {
    MEDICINE("medicine"),
    PROCEDURE("procedure"),
    SUPPORT_DIAGNOSTIC("supportDiagnostic");

    private final String value;

    ItemType(String value) {
        this.value = value;
    }

    public String getValue() {
        return value;
    }
}

public class Item {
    private String name;
    private double cost;
    private ItemType itemType;

    // Constructor por defecto
    public Item() {
    }

    // Constructor con parámetros
    public Item(String name, double cost, ItemType itemType) {
        this.name = name;
        this.cost = cost;
        this.itemType = itemType;
    }

    // Getters y Setters
    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public double getCost() {
        return cost;
    }

    public void setCost(double cost) {
        this.cost = cost;
    }

    public ItemType getItemType() {
        return itemType;
    }

    public void setItemType(ItemType itemType) {
        this.itemType = itemType;
    }

    // Método toString
    @Override
    public String toString() {
        return "Item{" +
                "name='" + name + '\'' +
                ", cost=" + cost +
                ", itemType=" + (itemType != null ? itemType.getValue() : "null") +
                '}';
    }
}

// Clase para órdenes médicas de medicinas
class MedicalOrder {
    private String medicine;
    private String dose;
    private String duration;

    // Constructor por defecto
    public MedicalOrder() {
    }

    // Constructor con parámetros
    public MedicalOrder(String medicine, String dose, String duration) {
        this.medicine = medicine;
        this.dose = dose;
        this.duration = duration;
    }

    // Getters y Setters
    public String getMedicine() {
        return medicine;
    }

    public void setMedicine(String medicine) {
        this.medicine = medicine;
    }

    public String getDose() {
        return dose;
    }

    public void setDose(String dose) {
        this.dose = dose;
    }

    public String getDuration() {
        return duration;
    }

    public void setDuration(String duration) {
        this.duration = duration;
    }

    // Método toString
    @Override
    public String toString() {
        return "MedicalOrder{" +
                "medicine='" + medicine + '\'' +
                ", dose='" + dose + '\'' +
                ", duration='" + duration + '\'' +
                '}';
    }
}

// Clase para órdenes de procedimientos
class ProcedureOrder {
    private String procedure;

    // Constructor por defecto
    public ProcedureOrder() {
    }

    // Constructor con parámetro
    public ProcedureOrder(String procedure) {
        this.procedure = procedure;
    }

    // Getter y Setter
    public String getProcedure() {
        return procedure;
    }

    public void setProcedure(String procedure) {
        this.procedure = procedure;
    }

    // Método toString
    @Override
    public String toString() {
        return "ProcedureOrder{" +
                "procedure='" + procedure + '\'' +
                '}';
    }
}

// Clase para órdenes de soporte diagnóstico
class SupportOrder {
    private String support;

    // Constructor por defecto
    public SupportOrder() {
    }

    // Constructor con parámetro
    public SupportOrder(String support) {
        this.support = support;
    }

    // Getter y Setter
    public String getSupport() {
        return support;
    }

    public void setSupport(String support) {
        this.support = support;
    }

    // Método toString
    @Override
    public String toString() {
        return "SupportOrder{" +
                "support='" + support + '\'' +
                '}';
    }
}

